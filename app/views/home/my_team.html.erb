  <div class="modal fade" id="contact_us" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel">

        <div role="document" class="modal-dialog">
		<div class="modal-content">
		  <div class="modal-header">
			<button aria-label="Close" data-dismiss="modal" class="close" type="button"><span aria-hidden="true">×</span></button>
			<h4 id="exampleModalLabel" class="modal-title">Your Details</h4>
		  </div>
		  <div class="modal-body">
			<%= form_for :contact_us,url: contact_us_path, :html=>{:class => "contact_us_form",id: "contact-form"} do |f| %>

			  <div class="form-group">
				<label for="exampleInputEmail1" class="label-txt">Name</label>
				 <%= f.text_field :name, autofocus: true, :class => "form-control form-c" %>
				 <span class="error-name error"></span>
			  </div>
			  <div class="form-group">
				<label class="label-txt" for="exampleInputEmail1">Email</label>
				<%= f.text_field :email, :class=> "form-control form-c"  %>
				<span class="error-email error"></span>
			  </div>
			   <div class="form-group">
				<label class="label-txt" for="exampleInputEmail1">Country</label>
				<%= country_list%>  

			  </div>
			  <div class="form-group">
				<label class="label-txt" for="exampleInputEmail1">Gender</label>
			  <div class="radio">
			  <label>
				<%= f.radio_button :gender, 'Male', :checked=> true %>
				Male
			  </label>
			</div>
			<div class="radio">
			  <label>
				<%= f.radio_button :gender, 'Female' %>
				Female
			  </label>
			</div>
			</div>
			<div class="form-group">
				<label class="label-txt" for="exampleInputEmail1">Author Code (if any)</label>
				<%= f.text_field :author_code, :class=> "form-control form-c"  %>
			</div>
			  
			
		  </div>
		  <div class="modal-footer put-center">
		  	<a href="#" class="btn btn-default btn-red" type="button" id = "contact_us_valid" onclick="$('.contact_us_form').submit();">Journey With Us</a>
      </div>
	   <% end %>
			</div>
		  </div>
	</div>  
	<!---------modal------------->
	<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel">
	  <div class="modal-dialog" role="document">
		<div class="modal-content">
		  <div class="modal-header">
			<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
			<h4 class="modal-title" id="exampleModalLabel">Your Details</h4>
		  </div>
		  <div class="modal-body">
			<form>
			  <div class="form-group">
				<label for="exampleInputEmail1" class="label-txt">Name</label>
				<input type="email" class="form-control" id="exampleInputEmail1" placeholder="">
			  </div>
			  <div class="form-group">
				<label for="exampleInputEmail1" class="label-txt">Email</label>
				<input type="email" class="form-control" id="exampleInputEmail1" placeholder="">
			  </div>
			   <div class="form-group">
				<label for="exampleInputEmail1" class="label-txt">Country</label>
				<select class="form-control">
				  <option>1</option>
				  <option>2</option>
				  <option>3</option>
				  <option>4</option>
				  <option>5</option>
				</select>
			  </div>
			  <div class="form-group">
				<label for="exampleInputEmail1"class="label-txt" >Gender</label>
			  <div class="radio">
			  <label>
				<input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked>
				Male
			  </label>
			</div>
			<div class="radio">
			  <label>
				<input type="radio" name="optionsRadios" id="optionsRadios2" value="option2">
				Female
			  </label>
			</div>
			</div>
			<div class="form-group">
				<label for="exampleInputEmail1"class="label-txt">Author ( If any)</label>
				<input type="email" class="form-control" id="exampleInputEmail1" placeholder="">
			</div>
			  
			</form>
		  </div>
		  <div class="modal-footer put-center">
			<button type="button" class="btn btn-default btn-red">Journey With Us</button>
		  </div>
		</div>
	  </div>
